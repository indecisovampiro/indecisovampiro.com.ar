  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "alpinejs": "^2.8.0",
    "fitty": "^2.3.3",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.2",
    "autoprefixer": "^10.2.3",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1"
  }
}
